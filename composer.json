{
  "require": {
    "slim/slim": "4.*",
    "slim/psr7": "^1.5",
    "selective/basepath": "^2.1",
    "php-di/php-di": "^6.4",
    "rakit/validation": "^1.4",
    "firebase/php-jwt": "^6.3",
    "squidit/slim-attribute-router": "^0.1.2",
    "doctrine/orm": "^2.13",
    "symfony/cache": "^6.1",
    "doctrine/annotations": "^1.13",
    "monolog/monolog": "^2.8",
    "symfony/polyfill-uuid": "^1.26",
    "cboden/ratchet": "^0.4.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "Modules",
      "App\\Framework\\Config\\": "Framework\\Config",
